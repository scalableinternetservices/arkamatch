<p id="notice"><%= notice %></p>

<h1>Gros</h1>

<table>
  <thead>
    <tr>
      <th>Version</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gros.each do |gro| %>
      <tr>
        <td><%= gro.version %></td>
        <td><%= link_to 'Show', gro %></td>
        <td><%= link_to 'Edit', edit_gro_path(gro) %></td>
        <td><%= link_to 'Destroy', gro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gro', new_gro_path %>
