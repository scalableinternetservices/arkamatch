<p id="notice"><%= notice %></p>

<h1>User Matches</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Match</th>
      <th>Num matches</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_matches.each do |user_match| %>
      <tr>
        <td><%= user_match %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Match', new_user_match_path %>
