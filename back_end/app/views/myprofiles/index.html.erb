<p id="notice"><%= notice %></p>

<h1>Myprofiles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @myprofiles.each do |myprofile| %>
      <tr>
        <td><%= link_to 'Show', myprofile %></td>
        <td><%= link_to 'Edit', edit_myprofile_path(myprofile) %></td>
        <td><%= link_to 'Destroy', myprofile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Myprofile', new_myprofile_path %>
